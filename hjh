        <input type="url" id="club" placeholder="Club" required>
        <input type="number" id="pace" placeholder="Pace" min="1" max="100" required>
        <input type="number" id="shooting" placeholder="Shooting" min="1" max="100" required>
        <input type="number" id="passing" placeholder="Passing" min="1" max="100" required>
        <input type="number" id="dribbling" placeholder="Dribbling" min="1" max="100" required>
        <input type="number" id="defending" placeholder="Defending" min="1" max="100" required>
        <input type="number" id="physical" placeholder="Physical" min="1" max="100" required>
        <input type="url" id="flag" placeholder="Flag URL" required>
        <input type="number" id="rating" placeholder="Rating" required>
